@font-face {
   font-family: myroboto;
   src: url(../fonts/Roboto/Roboto-Regular.ttf);
}

*{box-sizing:border-box;}

/* Let's get this party started */
::-webkit-scrollbar { width: 10px;} 
::-moz-scrollbar { width: 10px;} 
/* Track */
::-webkit-scrollbar-track {-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); background:#828282; } 
::-moz-scrollbar-track {-moz-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); background:#828282; } 
/* Handle */
::-webkit-scrollbar-thumb { background: #1b6ca1; }
::-moz-scrollbar-thumb { background: #1b6ca1; }
::-webkit-scrollbar-thumb:window-inactive {background: #298dcf; }  
::-moz-scrollbar-thumb:window-inactive {background: #298dcf; }  

body{font-family:myroboto;}

.width-auto{width:auto; float:left;}
.padding-10{padding:10px;}
.margin-btm-5{margin-bottom:5px;}
.margin-btm-10{margin-bottom:10px;}
.margin-btm-15{margin-bottom:15px;}
.margin-tp-5{margin-top:5px;}
.margin-tp-10{margin-top:10px;}
.margin-tp-15{margin-top:15px;}
.margin-rt-5{margin-right:5px;}
.margin-rt-10{margin-right:10px;}
.margin-rt-15{margin-right:15px;}
.margin-lt-5{margin-left:5px;}
.margin-lt-10{margin-left:10px;}
.margin-lt-15{margin-left:15px;}
input[type="button"], input[type="submit"]{background:#4682b4; color:#fff; padding:2px 10px;}
a:hover{text-decoration:none !important;}



.button1{background:#fff; padding:5px 10px; margin:0 10px; border-radius:5px; color:rgba(77, 141, 198, 1);} 
.no_border_radius{border-radius:0 !Important; }
body.bg1{background:url(../images/bg.jpg) no-repeat center top;}
.row{ margin-left:0px; margin-right:0px;}
#wrapper {padding-left: 40px;transition: all .4s ease 0s; -webkit-transition: all .4s ease 0s; -o-transition: all .4s ease 0s;-ms-transition: all .4s ease 0s; -moz-transition: all .4s ease 0s; height: 100%}
#sidebar-wrapper { margin-left: -150px; left: 40px; width: 150px; background: #525967;position: fixed; height: 100%;z-index: 10000; transition: all .4s ease 0s;-webkit-transition: all .4s ease 0s; -o-transition: all .4s ease 0s;-ms-transition: all .4s ease 0s; -moz-transition: all .4s ease 0s;}
.sidebar-nav {display: block;float: left;width: 150px;list-style: none; margin: 0; padding: 0;}
.nav_scroll{height:590px; overflow:auto;}
#page-content-wrapper {padding-left: 0;margin-left: 0;width: 100%;height: auto;}
#wrapper.active {padding-left: 150px;}
#wrapper.active #sidebar-wrapper {left:150px;}
#wrapper.active#wrapper{padding-left:170px;}
#wrapper.active#wrapper .main_content_section{width:86.8% !important;}



#page-content-wrapper { width: 100%;}

#sidebar_menu li a, .sidebar-nav li a {color: #999;display: block; float: left;text-decoration: none;width: 150px;background: #525967; border-top: 1px solid #4c525e;border-bottom: 1px solid #4c525e; -webkit-transition: background .5s;-moz-transition: background .5s;-o-transition: background .5s;-ms-transition: background .5s;transition: background .5s; padding:7px 0;}
.sidebar_name {padding-top: 25px;color: #dadee4; font-weight:bold; opacity: .7;}
.sidebar-nav li {line-height: 31px;text-indent: 9px;}
.sidebar-nav li a {color: #dadee4; font-weight:bold;display: block;text-decoration: none;}
.sidebar-nav li a:hover, .sidebar-nav li a.active {c150pxolor: #fff;background: rgba(38,46,56,0.99);text-decoration: none;}
.sidebar-nav li a:active, .sidebar-nav li a:focus { text-decoration: none;}
.sidebar-nav > .sidebar-brand {height: 65px;line-height: 60px;font-size: 18px;}
.sidebar-nav > .sidebar-brand a {color: #999999;}
.sidebar-nav > .sidebar-brand a:hover {color: #fff;background: none;}
.sidebar-nav > .active > a{background:#000;}
#main_icon{float:right;padding-right:10px; padding-top:0px;}
.sub_icon{float:right;padding-right:10px;padding-top:0px;}
.content-header {height: 65px;line-height: 65px;}
.content-header h1 {margin: 0; margin-left: 20px; line-height: 65px; display: inline-block;}



.form-signin {max-width: 380px;margin: 100px auto 0; background:#fff; box-shadow:0px 0px 15px rgba(0,0,0,.40); position:relative;}
.form-signin:before, .form-signin:after{top:25px; position:absolute;}
.form-signin:before{content:url(../images/box_left_label.png); left:-32px; float:left; }
.form-signin:after{content:url(../images/box_right_label.png); right:-32px; float:right;}

.form-signin .heading-logo-box{background:url(../images/logo_heading_icon.png) center no-repeat #08589d; padding:10px; background-position: right 20px top 10px;}
.form-signin .heading-logo-box h2, .form-signin .heading-logo-box h3{color:#fff; text-align:center; margin:0;}
.form-signin .heading-logo-box h2{font-weight:bold;}
.form-signin .heading-logo-box h3{font-size:13px; margin:3px 0 5px 0; position:relative;}
.form-signin .heading-logo-box h3:before, .form-signin .heading-logo-box h3:after{top:2px; position:absolute;}
.form-signin .heading-logo-box h3:before{content:url(../images/banner_left.png); left:70px; float:left; }
.form-signin .heading-logo-box h3:after{content:url(../images/banner_right.png); right:70px; float:right;}

.form-signin-inner-box{padding:20px 20px 50px 20px;}
.form-signin-inner-box input[type="text"], .form-signin-inner-box input[type="password"]{border:none !important; border-bottom:2px solid #e0e0e0 !important; border-radius:0; box-shadow:none; margin-bottom:20px;}


.for-pass a{color:#08589D;}
.color1{color:#08589D;}
.error_msg h2{font-size:11pt; margin:0;}

.main_page{}
.main_page .col-md-12{padding:0;}
.main_header{background:#03a9f4; width:100%; padding:25px 2%; float:left; /*box-shadow:0px 5px 10px rgba(151,151,151,.70); margin-bottom:20px;*/}
.main_header h4{margin:0; color:#FFFFFF;}
.main_header h4 li{list-style:none;}
.main_content_section{ padding:0% 0% 1% 0%;  position: fixed;  top: 98px; right:10px;  width:94.5%!important;  bottom: 0;  overflow: auto;}
.main_content_section h2{margin-top:0; font-weight:bold; font-size:20px; letter-spacing:1px;}

.table-view{table-layout:fixed;}
.table-view th{background:#b3e5fc; text-align:center;}
.table-view th, .table-view td{padding:15px; border:2px solid #a1cee2;}
.table-view td a{color:rgba(77, 141, 198, 1);}
.table-view td a:hover{color:#23527C;}

.link_red{color:#f26659;}
.link_blue{color:#1a67a5;}
.link_grey{color:#9c9c9c;}
.link_underline{text-decoration:underline;}

.form-list{list-style:none; margin:15px; padding:0;}
.form-list li{ width:100%; padding:0 10px; margin-bottom:10px; overflow:auto;}
.form-list li span{width:15%; float:left; margin-right:10px;}
/*.form-list li div{width:calc(85% - 15px); float:left;}*/
.form-list li em{font-style:normal; color:#f00;}
.form-list input[type="text"], .form-list input[type="file"], .form-list input[type="password"], .form-list textarea, .form-list select{width:200px; padding:5px;}

@media (max-width:767px) {
#wrapper {padding-left: 70px; transition: all .4s ease 0s;}
#sidebar-wrapper { left: 70px;}
#wrapper.active { padding-left: 150px;}
#wrapper.active #sidebar-wrapper { left: 150px; width: 150px; transition: all .4s ease 0s;}

	
}
@media (max-width:480px){
.form-signin:before, .form-signin:after{content:"";}	
.form-signin .heading-logo-box h3:before, .form-signin .heading-logo-box h3:after{content:"";}
}
#busy-indicator { display:none; }
